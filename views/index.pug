            li(role='presentation')
              a#profile-tab(href='#profile', role='tab', data-toggle='tab', aria-controls='profile', aria-expanded='false') Top viewed
            li(role='presentation')
              a#rating-tab(href='#rating', role='tab', data-toggle='tab', aria-controls='rating', aria-expanded='true') Top Rating
